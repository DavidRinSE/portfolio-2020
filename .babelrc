{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
          "#img": "./src/images",
          "#components": "./src/components",
          "#pages": "./src/pages"
      }
  }]
  ]
}